{
  "env": {
    "protractor": true,
    "jasmine": true
	},
	"globals": {
	},
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
		}
  },
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-underscore-dangle": "off",
    "no-console": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "linebreak-style": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "eol-last": 0
  }
}
